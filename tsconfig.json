{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "jsx": "react",
    "removeComments": true,
    "outDir": "./public"
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "public"]
}
